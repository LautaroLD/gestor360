{
  "expo": {
    "name": "gestor360",
    "slug": "gestor360",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "gestor360",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/favicon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundImage": "./assets/images/favicon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lautty_duran.gestor360"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/favicon.png",
          "resizeMode": "cover",
          "backgroundImage": "./assets/images/favicon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cd319728-d3bf-479e-a30b-e22feeebc58f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cd319728-d3bf-479e-a30b-e22feeebc58f"
    }
  }
}
